# Governance service base URL (e.g., http://localhost:8080/api)
GOVERNANCE_SERVICE=http://localhost:8080/api

# API token for the governance service
GOVERNANCE_AUTH=your_governance_service_token

# Rule ID to evaluate
RULE_ID=your_rule_id

# Path to the OpenAPI (OAS) file to analyze (must be accessible inside the container)
API_PATH=/data/openapi.yaml 